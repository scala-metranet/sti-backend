{
  "name": "levelup-express-backend",
  "version": "0.0.0",
  "description": "TypeScript + Knex + Postgresql + Express API Server",
  "author": "ichrome.fahdi@gmail.com",
  "license": "ISC",
  "scripts": {
    "start": "pnpm run build && cross-env NODE_ENV=production node dist/server.js",
    "start:tsc": "pnpm run build:tsc && cross-env NODE_ENV=production node dist/server.js",
    "start:nodemon": "cross-env NODE_ENV=production nodemon",
    "dev": "cross-env NODE_ENV=local nodemon",
    "dev:local": "cross-env NODE_ENV=local nodemon",
    "dev:test": "cross-env NODE_ENV=test nodemon",
    "build": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "test": "jest --forceExit --detectOpenHandles --watchAll --no-cache --verbose ",
    "test:deploy": "jest --forceExit --detectOpenHandles --no-cache --verbose ",
    "coverage": "jest --forceExit --detectOpenHandles --no-cache --silent --collect-coverage --forceExit",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "pnpm run lint -- --fix",
    "seed": "knex seed:run --knexfile=knexfile.ts --verbose",
    "migrate": "knex migrate:latest --knexfile=knexfile.ts --verbose",
    "rollback": "knex migrate:rollback --knexfile=knexfile.ts --verbose",
    "make:seeder": "knex seed:make --knexfile=knexfile.ts -x ts --env development",
    "make:migration": "knex migrate:make --knexfile=knexfile.ts -x ts --env development",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/core": "^7.20.2",
    "@supabase/supabase-js": "^2.2.2",
    "argon2": "^0.30.2",
    "axios": "^1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "docx-to-pdf-axios": "^1.1.4",
    "docxtemplater": "^3.37.9",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.0",
    "fs": "^0.0.1-security",
    "handlebars": "^4.7.7",
    "helmet": "^5.1.1",
    "hpp": "^0.2.3",
    "jose": "^4.11.1",
    "jsdom": "^23.2.0",
    "knex": "^2.3.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "nodemailer": "^6.8.0",
    "objection": "^3.0.1",
    "objection-js-soft-delete": "^3.0.3",
    "pg": "^8.8.0",
    "pizzip": "^3.1.4",
    "rate-limit-redis": "^4.1.2",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "slugify": "^1.6.6",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "xid-js": "^1.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.19",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-serve-static-core": "^4.17.31",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^28.1.8",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.45",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "node-config": "^0.0.2",
    "node-gyp": "^9.3.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.0",
    "supertest": "^6.3.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.1",
    "tsconfig-paths": "^4.1.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  }
}
